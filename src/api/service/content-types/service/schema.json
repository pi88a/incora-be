{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "url": {
      "type": "string"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "estimatedTime": {
      "type": "string",
      "required": true
    },
    "outcomes": {
      "type": "richtext",
      "required": true
    },
    "whyDoYouNeed": {
      "type": "component",
      "repeatable": false,
      "component": "common.list",
      "required": true
    },
    "bestSuitedFor": {
      "type": "component",
      "repeatable": false,
      "component": "common.list",
      "required": true
    },
    "workflow": {
      "type": "component",
      "repeatable": false,
      "component": "common.list",
      "required": true
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project"
    },
    "faq": {
      "type": "component",
      "repeatable": false,
      "component": "common.list",
      "required": true
    },
    "contactUs": {
      "type": "component",
      "repeatable": false,
      "required": true,
      "component": "common.contact-us"
    }
  }
}
