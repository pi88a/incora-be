{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "url": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::incora-worker.incora-worker"
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "impressions": {
      "displayName": "impression",
      "type": "component",
      "repeatable": false,
      "component": "article.impression",
      "required": true
    },
    "relatedArticles": {
      "displayName": "relatedArticles",
      "type": "component",
      "repeatable": false,
      "component": "article.related-articles",
      "required": false
    },
    "contactUs": {
      "type": "component",
      "repeatable": false,
      "component": "common.contact-us",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "articles"
    }
  }
}
